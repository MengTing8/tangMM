/* 卡片开始 */
/* pages/fetalWeight/fetalWeight.wxss */
.pageContainer {
    padding: 40rpx 42rpx;
    /* border-top: 2rpx solid #F1F1F1; */
    margin-bottom: 500rpx;
}

.listItem {
    background: rgba(255, 255, 255, 1);
    box-shadow: 0rpx 4rpx 16rpx 0rpx rgba(202, 225, 226, 0.38);
    border-radius: 12rpx;
    padding: 40rpx 40rpx 0;
    margin-bottom: 20rpx;
}

.item_content {
    border-bottom: 2rpx solid #F6F6F6;
}

.content_duration {
    margin-bottom: 20rpx;
    display: flex;
    justify-content: space-between;
}

.content_date {
    font-size: 24rpx;
    color: #999999;
}

.content_duration {
    font-size: 32rpx;
    color: rgba(15, 28, 54, 1);
    line-height: 44rpx;
}

.duration_date {
    font-size: 24rpx;
    color: rgba(205, 205, 205, 1);
    line-height: 34rpx;
}

.content_fetalWeight {
    margin-top: 30rpx;
}

.thisFetalWeight {
    font-size: 38rpx;
    font-weight: 500;
    color: rgba(254, 152, 0, 1);
    line-height: 52rpx;
}

.lastFetalWeight {
    font-size: 32rpx;
    font-weight: 500;
    color: rgba(153, 153, 153, 1);
    line-height: 44rpx;
}

.content_recording {
    font-size: 24rpx;
    color: #999999;
    line-height: 34rpx;
    margin-bottom: 20rpx;
    margin-left: 10rpx;
}

.item_partLength {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    font-size: 24rpx;
    color: rgba(153, 153, 153, 1);
    line-height: 44rpx;
    padding: 14rpx 0 12rpx;
}

.item_partLength view {
    /* margin-right: 40rpx; */
    width: 50%;
}

/* 卡片结束 */
.typeBtnContent {
    padding: 40rpx 10rpx 10rpx 10rpx;
}

.typeItemList {
    display: flex;
    align-items: center;
    text-align: center;
}

.typeBtnContent .typeItem {
    width: 150rpx;
    height: 30px;
    background: #F6F7F9;
    border-radius: 12rpx;
    color: #121212;
    font-size: 24rpx;
    margin: 0 20rpx;
    line-height: 58rpx;
}

.selectedItem {
    background: #FFF8E3 !important;
    color: #FE9800 !important;
}

/* 表单开始 */
.table {
    border: 0px solid darkgray;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    line-height: 88rpx;

    align-items: center;
}

.td {
    width: 40%;
    justify-content: center;
    text-align: center;
    border-right: 2rpx solid #EFF0F5;
    border-bottom: 2rpx solid #EFF0F5;


}

.bg-w {
    /* background: snow; */
}

.bg-g {

    /* background: #E6F3F9; */

}

.th {
    width: 40%;
    justify-content: center;
    background: #F6F7F9;
    color: #999;
    display: flex;
    line-height: 88rpx;
    align-items: center;
    border-right: 2rpx solid #EFF0F5;

}

.TableContent {
    width: 650rpx;
    margin: 30rpx auto;
    border: 2rpx solid #EFF0F5;
}

/* 表单结束 */
.selectedList {
    display: flex;
    padding: 30rpx;
    border-top: 2rpx solid #F6F6F6;
    z-index: 99999;

}

.checkedImg {
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
}

.mychartContent {
    margin: 0 auto;
    width: 100%;
    height: 1200rpx;
    /* margin-bottom: 50rpx; */
    position: relative;
    /* padding: 40rpx 100px !important; */
}



.titleContent {
    position: absolute;
    bottom: 2%;

    margin: 0 auto;
    padding: 0 40rpx;
}

.titleContentBox {
    display: flex;
    flex-wrap: wrap;

    align-items: center;
}

.legendBox {
    display: flex;
    justify-content: left;

    align-items: center;
    padding: 12rpx 12rpx;
    font-size: 24rpx;
    width: 30%;
}

.legendIcon {
    width: 26rpx;
    height: 12rpx;
    background: #296FD8;
    margin-right: 14rpx;
}

.goalBox {
    margin-top: 50rpx;
    display: flex;
    align-items: center;
    padding: 12rpx 12rpx;
    font-size: 24rpx;
}

.goalIcon {
    width: 26rpx;
    height: 26rpx;
    background: #296FD8;
    margin-right: 14rpx;
    border-radius: 50%;
}

