.tableList {
    display: flex;
    /* padding: 30rpx 10rpx; */
    color: #999999;
    font-size: 24rpx;
    border-top: 1px solid #EFF0F5;
    border-bottom: 1px solid #EFF0F5;
    padding: 18rpx 0;

}

.table {
    flex: 2;
    margin-right: 10rpx;

}

.tableTitle {
    margin-top: 58rpx;
    margin-right: 20rpx;
    margin-left: 20rpx;
    line-height: 58rpx;
}

.tr {
    display: flex;
    width: 100%;
    /* justify-content: right; */
    line-height: 58rpx;
    align-items: center;

}

.td {
    color: #999999;
    font-size: 24rpx;
}

.tha {
    flex: 3;
    /* justify-content: right; */
    display: flex;
    line-height: 58rpx;
    align-items: center;
}

.th {
    flex: 2;
    /* justify-content: right; */
    /* display: flex; */
    line-height: 58rpx;
    /* align-items: center; */
}

.ths {
    flex: 2;
    text-align: center;
}
