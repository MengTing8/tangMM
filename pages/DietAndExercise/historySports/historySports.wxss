.historyList {
    padding: 40rpx;
}
.ItemCard {
    padding: 40rpx;
    background: #FFFFFF;
    box-shadow: 0px 2px 8px 0px #CAE1E2;
    border-radius: 12rpx;
    margin-bottom: 30rpx;
}
.ItemCard_Time {
    color: #0F1C36;
    font-size: 34rpx;
    font-weight: normal;
    margin-bottom: 30rpx;
}
.ItemCard_Info {
    display: flex;
    color: #999999;
    font-size: 26rpx;
    line-height:42rpx;
}
.ItemCard_Info view{
    flex: 1;
}
.mychartContent {
    margin: 0 auto;
    width: 100%;
    height: 900rpx;
    margin-top: 50rpx;
    position: relative;
    /* padding: 40rpx 100px !important; */
}

.titleContentBox {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}

.legendBox {
    display: flex;
    align-items: center;
    padding: 12rpx 30rpx;
    font-size: 24rpx;
}

.legendIcon {
    margin-right: 14rpx;
}

.titleContent {
    position: absolute;
    right: 0;
    bottom: -5%;
    left: 0;
    display: flex;
    justify-content: center;
}

