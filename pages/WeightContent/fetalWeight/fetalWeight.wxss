/* pages/fetalWeight/fetalWeight.wxss */
.pageContainer{
  padding: 40rpx 42rpx;
  border-top: 2rpx solid #F1F1F1;
}
.listItem{
  background:rgba(255,255,255,1);
  box-shadow:0rpx 4rpx 16rpx 0rpx rgba(202,225,226,0.38);
  border-radius:12rpx;
  padding: 40rpx 40rpx 0;
  margin-bottom: 20rpx;
}
.item_content{
  border-bottom: 2rpx solid #F6F6F6;
}
.content_duration,.content_fetalWeight{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content_duration{
  font-size:32rpx;
  color:rgba(15,28,54,1);
  line-height:44rpx;
}
.duration_date{
  font-size:24rpx;
  color:rgba(205,205,205,1);
  line-height:34rpx;
}
.content_fetalWeight{
  margin-top: 30rpx;
}
.thisFetalWeight{
  font-size:38rpx;
  font-weight:500;
  color:rgba(254,152,0,1);
  line-height:52rpx;
}
.lastFetalWeight{
  font-size:32rpx;
  font-weight:500;
  color:rgba(153,153,153,1);
  line-height:44rpx;
}
.content_recording{
  display: flex;
  justify-content: space-between;
  font-size:24rpx;
  color:rgba(205,205,205,1);
  line-height:34rpx;
  margin-bottom: 20rpx;
}
.item_partLength{
  display: flex;
  justify-content: space-between;
  font-size:24rpx;
  color:rgba(153,153,153,1);
  line-height:44rpx;
  padding: 14rpx 0 12rpx;
}
