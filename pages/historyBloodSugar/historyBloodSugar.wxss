.TableContent{
    font-size: 24rpx;
    padding: 0 20rpx 60rpx 20rpx;
}
.header{
    display: flex;
    /* justify-content: space-between; */
    align-items: center;
}
.header view{
    flex: 4;
    text-align: center;
    border-right: 2rpx solid #dddddd;
}
.header view:nth-child(1) {
   flex: 3.12;
   /* flex: 1.46; */
   border-right: 2rpx solid #dddddd;
   
}
.header view:nth-child(2) {
    border-left: 2rpx solid #dddddd;
}
.header view:nth-child(5) {
    flex: 1.33;
    border-right:none;
}
.header2 {
    display: flex;
    /* justify-content: space-between; */
    align-items: center;
    line-height: 48rpx;
}
.header2 view {
    flex: 4;
    text-align: center;
}

.header2 view:nth-child(1) {
    flex: 9.5;
    /* flex: 4.5; */
}
.header2 view:nth-child(1),
.header2 view:nth-child(4),
.header2 view:nth-child(7),
.header2 view:nth-child(10) {
  border-right: 2rpx solid #dddddd;
}
.GlucoseListCard {
    background: #FFFFFF;
    box-shadow: 0px 2px 8px 0px #CAE1E2;
    border-radius: 12rpx;
    padding: 10rpx;
    margin-top: 30rpx;
}
.border{
    border-bottom: 2rpx solid #dddddd;
}
.bloodSugarInfoLIst {
    padding: 40rpx;
}

.listCard {
     background: #FFFFFF;
     box-shadow: 0px 2px 8px 0px #CAE1E2;
     border-radius: 12rpx;
     padding: 40rpx;
     margin-top: 30rpx;
}

.listCard_top {
     color: #0F1C36;
    font-size: 34rpx;
    font-weight: normal;
    margin-bottom: 30rpx;
     display: flex;
     justify-content: space-between;
}

/* .listCard_bottom {
    color: #999999;
