.listCard_bottom_content {
     display: flex;
     justify-content: space-between;
     flex-wrap: wrap; 
     margin-bottom: 15rpx;
     line-height: 40rpx;
}

.listCard_bottom_content view {
   width: 50%;
   /* flex: 1; */
}

.mychartContent { 
    margin: 0 auto; 
    width: 100%; 
    height: 800rpx; 
    position: relative; 
    /* padding: 40rpx 100rpx !important; */ 
} 
 
.titleContentBox { 
    display: flex; 
    flex-wrap: wrap; 
    align-items: center; 
} 
 
.legendBox { 
    display: flex; 
    font-size: 24rpx; 
    align-items: center; 
    padding: 12rpx 30rpx; 
} 
 
.legendIcon { 
    margin-right: 14rpx; 
} 
 
.titleContent { 
    margin-top: 20rpx; 
} 
 
.typeBtnContent { 
    padding: 40rpx 10rpx 0 10rpx; 
} 
 
.typeItemList { 
    display: flex; 
    text-align: center; 
    flex-wrap: wrap; 
} 
 
.typeBtnContent .typeItem { 
    width: 150rpx; 
    height: 30px; 
    background: #F6F7F9; 
    border-radius: 12rpx; 
    color: #121212; 
    font-size: 24rpx; 
    margin: 0 10rpx 20rpx; 
    line-height: 30px; 
} 
 
.selectedItem { 
    background: #FFF8E3 !important; 
    color: #FE9800 !important; 
}